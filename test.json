  "testRunner": {
    "name": "SetupX Test Runner",
    "version": "1.0.0",
    "description": "Automated test runner for SetupX components",
    "functions": {
      "runComponentTest": {
        "description": "Run tests for a specific component",
        "usage": "runComponentTest [component-name]"
      },
      "runTestSuite": {
        "description": "Run tests for a specific test suite",
        "usage": "runTestSuite [suite-name]"
      },
      "runAllTests": {
        "description": "Run all component tests",
        "usage": "runAllTests"
      },
      "runCleanup": {
        "description": "Run cleanup for a specific component",
        "usage": "runCleanup [component-name]"
      }
    }
