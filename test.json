    "chocolatey": {
      "name": "chocolatey",
      "displayName": "Chocolatey",
      "testSteps": [
        {
          "step": 1,
          "action": "Check Chocolatey Installation",
          "command": "choco --version",
          "expectedOutput": "Chocolatey v",
          "timeout": 30
        },
        {
          "step": 2,
          "action": "Test Package Search",
          "command": "choco search git --limit-output",
          "expectedOutput": "git",
          "timeout": 30
        },
        {
          "step": 3,
          "action": "Test Package Installation",
          "command": "choco install 7zip -y",
          "expectedOutput": "7zip installed successfully",
          "timeout": 60
        }
      ],
      "cleanupSteps": [
        {
          "step": 1,
          "action": "Uninstall Test Package",
          "command": "choco uninstall 7zip -y"
        }
      ]
    },
