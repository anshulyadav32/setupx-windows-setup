  "installation": {
    "methods": {
      "one-command": {
        "description": "Single command installation from GitHub",
        "command": "Invoke-RestMethod -Uri https://raw.githubusercontent.com/anshulyadav32/setupx-windows-setup/main/install.ps1 | Invoke-Expression",
        "requirements": ["PowerShell 5.1+", "Internet connection"]
      },
      "force-installation": {
        "description": "Force installation without prompts",
        "command": "iwr https://raw.githubusercontent.com/anshulyadav32/setupx-windows-setup/main/install.ps1 | iex -Force",
        "requirements": ["PowerShell 5.1+", "Internet connection"]
      },
      "manual-installation": {
        "description": "Clone repository and install locally",
        "commands": [
          "git clone https://github.com/anshulyadav32/setupx-windows-setup.git",
          "cd setupx-windows-setup",
          ".\\install.ps1"
        ],
        "requirements": ["Git", "PowerShell 5.1+"]
      }
    },
    "requirements": {
      "os": "Windows 10/11",
      "powershell": "5.1 or later",
      "internet": "Required for installation",
      "admin": "Optional (some components work better as regular user)"
    }
  },
  "troubleshooting": {
    "common-issues": {
      "404-not-found": {
        "problem": "Using old installation URL",
        "solution": "Use the new install.ps1 URL from the repository"
      },
      "setupx-command-not-found": {
        "problem": "PATH not updated in current session",
        "solution": "Restart PowerShell or run 'refreshenv'"
      },
      "module-not-found": {
        "problem": "Installation didn't complete properly",
        "solution": "Reinstall with force mode: 'iwr .../install.ps1 | iex -Force'"
      },
      "installation-script-not-found": {
        "problem": "Component scripts not downloaded properly",
        "solution": "Use the latest install.ps1 (now fixed)"
      },
      "emoji-parsing-errors": {
        "problem": "PowerShell version compatibility",
        "solution": "Use the latest install.ps1 (emoji-free version)"
      }
    }
  },
